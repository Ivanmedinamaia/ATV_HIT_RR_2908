.data
    s_text: .asciiz "s = "
    s_value: .asciiz "\n"
.text
    .globl main
main:
    
    li $t0, 0      
    li $t1, 5       
    li $t2, 1       

    loop:
        bge $t0, $t1, end    # Se i >= j, sair do loop
        add $t2, $t2, $t0    # s = s + i
        addi $t0, $t0, 1     # i++
        j loop               # Voltar para o in√≠cio do loop

    li $v0, 4
    la $a0, s_text
    syscall

    li $v0, 1
    move $a0, $t2
    syscall

    li $v0, 10
    syscall

end:
